(
(:message tutorial::person
  (:enum tutorial::phone-type
	 (:mobile 0)
	 (:home   1)
	 (:work   2))
  (:message tutorial::phone-number
    (:field tutorial::number :string 1 :required t)
    (:field tutorial::type   tutorial::person-phone-type 2 :optional t))
  (:field tutorial::name :string                1 :required t)
  (:field tutorial::id  :int32                  2 :required t)
  (:field tutorial::email :string               3 :optional t)
  (:field tutorial::phone tutorial::person-phone-number 4 :repeated t))

 (:message tutorial::address-book
   (:field tutorial::person tutorial::person    1 :repeated t))
)
